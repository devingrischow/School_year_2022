--GET THE TYPE OF DATA 
SELECT TYPEOF( 1 + 1 );
SELECT TYPEOF(1 + 1.0);
SELECT TYPEOF('pandabear');
SELECT TYPEOF('panda' + 'koalabear');

--Division Examples:
SELECT 1/2;
SELECT 1.0/2;
SELECT CAST(1 AS REAL) / 2;
SELECT 17/5;
SELECT 17/5, 17%5;

--ROUNDING NUMBERS
SELECT 2.555555;
SELECT ROUND(2.55555);
SELECT ROUND(2.55555, 3);
SELECT ROUND(2.55555, 0);




